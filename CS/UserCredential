[Serializable]
    public class UserCredential
    {
        public String Username { set; get; }
        public String Fullname { set; get; }
        public string EmployeeID { get; set; }
        public string HelpFileFolderName { get; set; }

        public int? DistributorUID { get; set; }
        public int? PriceListUID1 { get; set; }
        public int PriceListUID3 { get; set; }

        public String Distributor { set; get; }
        public int EmployeeUID { get; set; }
        public int AgentUID { get; set; }
        public int LoginUID { get; set; }
        public int SiteUID { get; set; }
        public int? ProfileUID { get; set; }
        public int? ChannelUID { get; set; }

        public EmployeeBL.DesignationEnum Designation { get; set; }
        
        public bool Locked { set; get; }

    }
